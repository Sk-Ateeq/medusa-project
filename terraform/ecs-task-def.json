[
  {
    "name": "medusa",
    "image": "docker.io/medusajs/medusa:latest",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 9000,
        "hostPort": 9000,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "NODE_ENV",
        "value": "production"
      }
    ]
  }
]
